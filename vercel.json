{
    "version": 2,
    "builds": [{ "src": "dist/index.js", "use": "@vercel/node" }],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/index.js"
      }
    ],
    "crons": [
    {
      "path": "/api/every-minute",
      "schedule": "* * * * *"
    }
  ],
    "public": true,
     "buildCommand": "npm run build",
        "installCommand": "npm install"
      
  }